{
  "name": "sauce-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --spec 'cypress/e2e/features/**/*.feature'",
    "test": "npm run cypress",
    "report:merge": "npx mochawesome-merge mochawesome-report/json/*.json > mochawesome.json",
    "report:generate": "npx mochawesome-report-generator mochawesome.json --reportDir mochawesome-report",
    "report:clean": "rm -rf mochawesome-report/assets mochawesome-report/json mochawesome-report/mochawesome.html mochawesome.json",
    "dashboards:restore": "node scripts/restore-dashboards.js",
    "test:report": "npm run report:clean && npm run cypress:run && npm run report:merge && npm run report:generate && npm run dashboards:restore"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^23.2.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.7",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "cypress": "^15.6.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^6.0.0"
  }
}
